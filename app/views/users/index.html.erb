<head>
  <% provide(:title, "Users' Index | Mean Review") %>
</head>
<body>
  <div class="interface">
    <br /><h2 class="title">User's Index</h2>
    <div class="users_index">
      <% @users.order(:username).each.sort.uniq do |user| %>
        <%= link_to user.username, user_path(user) %> |
        <%= link_to "Delete", user_path(user), data: { confirm: "Are you sure?" }, method: :delete %><br /><br />
      <% end %>
    </div><br />
    <div class="back">
      <%= link_to 'Back', :back, class: 'btn btn-default' %>
    </div><br /><br />
  </div>
</body>
